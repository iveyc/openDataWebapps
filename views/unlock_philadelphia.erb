									<label><img src="images/stairsredsm.png" width="32" height="32" alt="Stairs">
										<input type="checkbox" name="filterOptions" id="filterStairsOnly" value="StairsOnly" checked>
										Stairs Only</label>
								</div>
								<div class="radio">
									<label> More filters...</label>
								</div>
							</div>
